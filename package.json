{
  "name": "coursehunters-downloader",
  "version": "1.0.2",
  "description": "A plugin for downloading video from https://coursehunters.net",
  "main": "dist/index.min.js",
  "scripts": {
    "prep": "npm i -D @babel/cli @babel/core @babel/preset-env del-cli uglify-es",
    "babel": "./node_modules/.bin/babel src/index.js -o dist/index.tr.js",
    "min": "./node_modules/.bin/uglifyjs dist/index.tr.js -c -m -o dist/index.min.js",
    "del": "./node_modules/.bin/del-cli dist/index.tr.js",
    "build": "npm run babel && npm run min && npm run del"
  },
  "keywords": [
    "npm",
    "coursehunters",
    "downloader",
    "video"
  ],
  "author": "Fewed",
  "homepage": "https://github.com/Fewed/coursehunters-downloader",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "del-cli": "^1.1.0",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
